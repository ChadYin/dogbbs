<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="default" extends="struts-default">
		<action name="login" class="userAction" method="login">
			<result name="success">/index.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="regist" class="userAction" method="regist">
			<result name="success">/login.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="checkuname" class="userAction" method="checkuname">
		</action>
		<action name="updatepassword" class="userAction" method="updatepassword">
			<result name="success">/login.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="updateuserinfo" class="userAction" method="updateuserinfo">
			<result name="success">/index.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="userlist" class="userAction" method="userlist">
			<result name="success">/userlist.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="forbid" class="userAction" method="forbid">
			<result name="success">/userlist.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="forbiduserlist" class="userAction" method="forbiduserlist">
			<result name="success">/forbiduserlist.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="outforbid" class="userAction" method="outforbid">
			<result name="success">/forbiduserlist.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="searchuser" class="userAction" method="searchuser">
			<result name="success">/userlist.jsp</result>
			<result name="input">/error.jsp</result>
		</action>


		<action name="sectionlist" class="sectionAction" method="sectionlist">
			<result name="success">/index.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="addsection" class="sectionAction" method="addsection">
			<result name="success" type="redirect">/index.jsp</result>
			<result name="input">/error.jsp</result>
		</action>

		<action name="searchtopic" class="topicAction" method="searchtopic">
			<result name="success">/topic.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="topiclist" class="topicAction" method="topiclist">
			<result name="success">/topic.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="addtopic" class="topicAction" method="addtopic">
			<result name="success">/topic.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="deletetopic" class="topicAction" method="deletetopic">
			<result name="success">/topic.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="usertopic" class="topicAction" method="usertopic">
			<result name="success">/usertopic.jsp</result>
			<result name="input">/error.jsp</result>
		</action>


		<action name="replylist" class="replyAction" method="replylist">
			<result name="success">/reply.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="addreply" class="replyAction" method="addreply">
			<result name="success">/reply.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="deletereply" class="replyAction" method="deletereply">
			<result name="success">/reply.jsp</result>
			<result name="input">/error.jsp</result>
		</action>
		<action name="userreply" class="replyAction" method="userreply">
			<result name="success">/userreply.jsp</result>
			<result name="input">/error.jsp</result>
		</action>


	</package>
</struts>